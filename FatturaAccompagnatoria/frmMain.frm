Private Sub AGGIORNAMENTO_RIGHE_PER_AGENTE(IDAgente As Long, NomeAgente As String, Agente As String)
On Error GoTo ERR_AGGIORNAMENTO_RIGHE_PER_AGENTE
Dim Link_Regola_provv As Long
Dim I As Long

Link_Regola_provv = GET_LINK_REGOLA_PROVV_AGE(IDAgente, TheApp.IDFirm)

If Link_Regola_provv = 0 Then
    MsgBox "Aggiornamento non eseguito poichè l'agente non ha impostata una regola di provvigione predefinita", vbInformation, "Aggiornamento agente"
    Exit Sub
