                If Link_RigaConferimento = 0 Then
                    Me.CDSocioFatt.Load GET_LINK_ANA_FATT(Me.ACSSocio.IDAnagrafica)
                End If
                rs.CloseResultset
                Set rs = Nothing
            End If
            
        End If
    End If
End If
