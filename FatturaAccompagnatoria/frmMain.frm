Private Sub sbCalcolaImportoLiquidazione(ImpImballo As Double)
Dim ImportoImballo As Double
Dim ImportoImballoUnitario As Double

ImportoImballo = ImpImballo

ImportoImballoUnitario = 0

'ImportoImballo = GET_PREZZO_IMBALLO(ImportoImballo)
If Me.txtQta_UM.Value > 0 Then
    ImportoImballoUnitario = (ImportoImballo * Me.txtColli.Value) / Me.txtQta_UM.Value

    If Me.chkImportoImballoInArticolo.Value = vbChecked Then
        Me.txtImportoLiq.Value = -Abs(ImportoImballoUnitario)  'Me.txtImportoLiq.Value - Abs(ImportoImballoUnitario)
        
        Me.txtVariazionePrezzoImballo.Value = ImportoImballoUnitario
    Else
        Me.txtImportoLiq.Value = 0
        
        Me.txtVariazionePrezzoImballo.Value = 0
    End If
Else
    Me.txtImportoLiq.Value = FormatNumber(0, 4)
    Me.txtVariazionePrezzoImballo.Value = 0
End If


If LINK_UM_LIQUIDAZIONE = Link_UMCoop Then
    If Moltiplicatore > 0 Then
        Me.txtImportoLiq.Value = Me.txtImportoLiq.Value / Moltiplicatore
    Else
        Me.txtImportoLiq.Value = Me.txtImportoLiq.Value
    End If
Else
    If Me.txtQtaLiquidazione.Value > 0 Then
        Me.txtImportoLiq.Value = (Me.txtImportoLiq.Value * Me.txtQta_UM.Value) / Me.txtQtaLiquidazione.Value
    Else
        Me.txtImportoLiq.Value = Me.txtImportoLiq.Value
    End If
End If

