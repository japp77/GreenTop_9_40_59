sSQL = sSQL & "WHERE IDFiliale=" & TheApp.IDFirm

Set rs = Cn.OpenResultset(sSQL)

If rs.EOF Then
    GET_TIPO_IMPORTO_ARTICOLO_DA_LIQUIDAZIONE = 0
Else
    If fnNotNullN(rs!IDTipoImportoArticolo) = 3 Then
        GET_TIPO_IMPORTO_ARTICOLO_DA_LIQUIDAZIONE = 1
