Private Sub CalcoloPesoNetto()
On Error Resume Next
Dim ArrayPesoNetto() As String
Dim PesoNetto As Double
Dim Decimal_PesoNetto As Double
Dim Tara As Double

If Me.CDImballo.KeyFieldID > 0 Then
    'Me.txtTara.Value = (Me.txtColli.Value * Me.txtTaraUnitaria.Value) + (Me.txtColli.Value * NConfImballoIVGamma * TaraConfIVGamma)
    Me.txtTara.Value = (Me.txtColli.Value * Me.txtTaraUnitaria.Value) + (Me.txtColli.Value * Me.txtNumeroConfImballo.Value * Me.txtTaraConfImballo.Value)
End If

'Me.txtPesoNetto.Value = Me.txtPesoLordo.Value - Me.txtTara.Value

'If TIPO_PESO_ARTICOLO <= 1 Then
'    Me.txtPesoNetto.Value = Me.txtPesoLordo.Value - Me.txtTara.Value
'Else
'    Me.txtPesoLordo.Value = Me.txtTara.Value + Me.txtPesoNetto.Value
'End If

If (Me.chkArticoloPedana.Value) = vbUnchecked Then
    If TIPO_PESO_ARTICOLO <= 1 Then
        Me.txtPesoNetto.Value = Me.txtPesoLordo.Value - Me.txtTara.Value
    Else
        If PESO_LORDO = 0 Then
            Me.txtPesoNetto.Value = Me.txtPesoLordo.Value - Me.txtTara.Value
        Else
            Me.txtPesoLordo.Value = Me.txtTara.Value + Me.txtPesoNetto.Value
        End If
    End If
Else
    Me.txtPesoLordo.Value = Me.txtColli.Value * Me.txtTaraUnitaria.Value
    Me.txtPesoNetto.Value = 0
End If
Select Case Link_Arrotondamento
    Case 1 'Nessun arrotondamento
        Me.txtPesoNetto.Value = Me.txtPesoLordo.Value - Me.txtTara.Value
    Case 2 'Matematico
        Me.txtPesoNetto.Value = fnRoundChange(Me.txtPesoNetto.Value, 1, 3)
        Me.txtTara.Value = Me.txtPesoLordo.Value - Me.txtPesoNetto.Value
    Case 3 'Difetto
        Me.txtPesoNetto.Value = fnRoundDown(Me.txtPesoNetto.Value)
        Me.txtTara.Value = Me.txtPesoLordo.Value - Me.txtPesoNetto.Value
    Case 4 'Eccesso
        Me.txtPesoNetto.Value = fnRoundUp(Me.txtPesoNetto.Value)
        Me.txtTara.Value = Me.txtPesoLordo.Value - Me.txtPesoNetto.Value
End Select

End Sub

